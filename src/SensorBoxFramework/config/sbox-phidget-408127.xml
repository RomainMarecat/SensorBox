<?xml version="1.0" encoding="UTF-8" ?>
<sensorBox>
        <boxId><TMPL_VAR name="deviceid"></boxId>
        <owner><TMPL_VAR name="user"></owner>
        <location><TMPL_VAR name="location"></location>
        <endpoint><TMPL_VAR name="endpoint"></endpoint>
    <sensors>
		<sensor>
	        <sensorId>humidity</sensorId>
	        <device>Phidgets1125_Humidity</device>
	        <sensortype>Humidity</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>0</port>
	        </accessMethod>
	        <samplingRate>10000</samplingRate>
	    </sensor>
    	<sensor>
	        <sensorId>temperature</sensorId>
	        <device>Phidgets1125_Temperature</device>
	        <sensortype>Temperature</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>1</port>
	        </accessMethod>
	        <samplingRate>10000</samplingRate>
	    </sensor>
	    <sensor>
	        <sensorId>light</sensorId>
	        <device>Phidgets1127_Light</device>
	        <sensortype>Light</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>2</port>
	        </accessMethod>
	        <samplingRate>1000</samplingRate>
	    </sensor>
	    <sensor>
	        <sensorId>pressure</sensorId>
	        <device>Phidgets1141_AbsolutePressure</device>
	        <sensortype>GasPressure</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>3</port>
	        </accessMethod>
	        <samplingRate>10000</samplingRate>
	    </sensor>

	    
		<sensor>
	        <sensorId>motion</sensorId>
	        <device>Phidgets1111_Motion</device>
	        <sensortype>Motion</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>4</port>
	        </accessMethod>
	        <samplingRate>10</samplingRate>
	    </sensor>    
        	    
		<sensor>
	        <sensorId>sound</sensorId>
	        <device>Phidgets1133_Sound</device>
	        <sensortype>Sound</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>5</port>
	        </accessMethod>
	        <samplingRate>10000</samplingRate>
	    </sensor>
	    <sensor>
	        <sensorId>vibration</sensorId>
	        <device>Phidgets1104_Vibration</device>
	        <sensortype>Vibration</sensortype>
	        <accessMethod>
	            <interface>IFKit</interface>
	            <serial>408127</serial>
	            <port>6</port>
	        </accessMethod>
	        <samplingRate>10000</samplingRate>
	    </sensor>





	    <!-- httpのパターンならこんな感じ -->
